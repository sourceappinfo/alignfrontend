        "family": "Raleway-Regular"
      },
      {
        "asset": "./src/assets/fonts/Raleway-Medium.ttf",
        "family": "Raleway-Medium"
      },
      {
        "asset": "./src/assets/fonts/Raleway-SemiBold.ttf",
        "family": "Raleway-SemiBold"
      },
      {
        "asset": "./src/assets/fonts/Raleway-Bold.ttf",
